#include "LCD.h"
#include "Keypad.h"
#include "ADC.h"
int main(void)
{
	uint8 data;
	LCD_Init();
	ADC_Init();
	while (1)
	{
		data = ADC_read(0);
		LCD_IntegerToString(data);
		LCD_GoToRowCol(0,10);
		LCD_SendCharacter('C');
	}
	return 0;
}


